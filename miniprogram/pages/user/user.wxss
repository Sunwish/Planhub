.user-pic{
  display: flex;
  flex-direction: column;
  margin-top: 120rpx;
}
.user-name{
  display: flex;
  font-size:47rpx;
  font-weight:bold; 
  margin-top: 40rpx;
}

.function van-cell{
  display: flex;
  width: 700rpx;
  flex:1;
}

.s1{
  margin-top: 70rpx;
}
.s2{
  margin-top: 30rpx;
  display: flex;
  flex:1;
}
.cell-icon{
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  height: 24px;
  height: var(--cell-line-height, 24px);
  font-size: 16px;
  font-size: var(--cell-icon-size, 16px);
  margin-right: 10px;
}
.cell-switch{
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  height: 24px;
  height: var(--cell-line-height, 24px);
  font-size: 16px;
  font-size: var(--cell-icon-size, 16px);
  margin-left: 5px;
}

.contact-btn {
  display: inline-block;
  position: absolute;
  width: 100%;
  margin-left: 0rpx;
  padding-left: 0rpx;
  text-align: left;
}


